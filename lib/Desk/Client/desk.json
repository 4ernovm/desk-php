{
    "name": "Desk.com",
    "description": "",
    "apiVersion": 2,
    "operations": {
        "base": {
            "responseType": "model",
            "parameters": {
                "embed": {
                    "description": "Which relationships should be embedded in the response",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "type": "string",
                        "enum": []
                    },
                    "location": "query"
                }
            }
        },


        "list": {
            "extends": "base",
            "httpMethod": "GET",
            "responseClass": "Page",
            "parameters": {
                "per_page": {
                    "description": "Number of entries returned per page",
                    "type": "integer",
                    "minimum": 1,
                    "location": "query"
                },
                "page": {
                    "description": "The page number of results to retrieve",
                    "type": "integer",
                    "minimum": 1,
                    "location": "query"
                }
            },
            "data": {
                "links": {
                    "self": {
                        "operation": "$self",
                        "pattern": "#\\?page=(?P<page>[0-9]+)&per_page=(?P<per_page>[0-9]+)$#"
                    },
                    "first": {
                        "operation": "$self",
                        "pattern": "#\\?page=(?P<page>[0-9]+)&per_page=(?P<per_page>[0-9]+)$#"
                    },
                    "last": {
                        "operation": "$self",
                        "pattern": "#\\?page=(?P<page>[0-9]+)&per_page=(?P<per_page>[0-9]+)$#"
                    },
                    "next": {
                        "operation": "$self",
                        "pattern": "#\\?page=(?P<page>[0-9]+)&per_page=(?P<per_page>[0-9]+)$#"
                    },
                    "previous": {
                        "operation": "$self",
                        "pattern": "#\\?page=(?P<page>[0-9]+)&per_page=(?P<per_page>[0-9]+)$#"
                    }
                }
            }
        },
        "show": {
            "extends": "base",
            "httpMethod": "GET",
            "parameters": {
                "id": {
                    "description": "The Desk ID of the item to retrieve",
                    "required": true,
                    "type": "integer",
                    "minimum": 1,
                    "location": "uri"
                }
            }
        },
        "create": {
            "extends": "base",
            "httpMethod": "POST"
        },
        "update": {
            "extends": "base",
            "httpMethod": "PATCH"
        },
        "delete": {
            "extends": "base",
            "httpMethod": "DELETE"
        },
        "search": {
            "extends": "base",
            "httpMethod": "GET"
        },


        "ListCustomers": {
            "extends": "list",
            "summary": "Retrieve a paginated list of all customers",
            "uri": "customers",
            "data": {
                "embeds": {
                    "entries": {
                        "model": "CustomerModel"
                    }
                }
            }
        },
        "ShowCustomer": {
            "extends": "show",
            "summary": "Retrieve a single customer by ID",
            "uri": "customers/{id}",
            "responseClass": "CustomerModel",
            "data": {
                "links": {
                    "self": {
                        "operation": "ShowCustomer",
                        "pattern": "#/api/v2/customers/(?P<id>[0-9]+)$#"
                    }
                }
            }
        },


        "ListUsers": {
            "extends": "list",
            "summary": "Retrieve a paginated list of all users",
            "uri": "users",
            "data": {
                "embeds": {
                    "entries": {
                        "model": "UserModel"
                    }
                }
            }
        },
        "ShowUser": {
            "extends": "show",
            "summary": "Retrieve a single user by ID",
            "uri": "users/{id}",
            "responseClass": "UserModel",
            "data": {
                "links": {
                    "self": {
                        "operation": "ShowUser",
                        "pattern": "#/api/v2/users/(?P<id>[0-9]+)$#"
                    }
                }
            }
        }
    },
    "models": {
        "Page": {
            "type": "object",
            "properties": {
                "total_entries": {
                    "description": "Total number of entries on all pages of the result",
                    "location": "json",
                    "type": "integer"
                }
            }
        },
        "CustomerModel": {
            "data": {
                "deskClass": "customer"
            },
            "type": "object",
            "properties": {
                "first_name": {
                    "description": "The customer's first name",
                    "location": "json",
                    "type": "string"
                },
                "last_name": {
                    "description": "The customer's last name",
                    "location": "json",
                    "type": "string"
                },
                "company": {
                    "description": "The customer's company",
                    "location": "json",
                    "type": "string"
                },
                "title": {
                    "description": "The customer's title",
                    "location": "json",
                    "type": "string"
                },
                "external_id": {
                    "description": "The customer's unique external identifier to reference this customer in an external system",
                    "location": "json",
                    "type": "string"
                },
                "background": {
                    "description": "Any background information for the customer",
                    "location": "json",
                    "type": "string"
                },
                "locked_until": {
                    "description": "Time at which the lock on the customer will expire",
                    "location": "json",
                    "type": "string"
                },
                "created_at": {
                    "description": "Time at which the customer was created",
                    "location": "json",
                    "type": "string"
                },
                "updated_at": {
                    "description": "Time at which the customer was last updated at",
                    "location": "json",
                    "type": "string"
                },
                "emails": {
                    "description": "Array of e-mail objects, each specifying the contact type and value of the e-mail address",
                    "location": "json",
                    "type": "array",
                    "items": {
                    }
                },
                "phone_numbers": {
                    "description": "Array of phone number objects, each specifying the contact type and value of the phone number",
                    "location": "json",
                    "type": "array",
                    "items": {
                    }
                },
                "addresses": {
                    "description": "Array of address objects, each specifying the contact type and value of the location",
                    "location": "json",
                    "type": "array",
                    "items": {
                    }
                },
                "custom_fields": {
                    "description": "Associative array of values for custom fields",
                    "location": "json",
                    "type": "array",
                    "items": {
                    }
                }
            }
        },
        "UserModel": {
            "type": "object",
            "properties": {
                "name": {
                    "description": "Name of the user",
                    "location": "json",
                    "type": "string"
                },
                "public_name": {
                    "description": "Public facing name of the user",
                    "location": "json",
                    "type": "string"
                },
                "email": {
                    "description": "User's email",
                    "location": "json",
                    "type": "string"
                },
                "level": {
                    "description": "User's permission level",
                    "location": "json",
                    "type": "string"
                }
            }
        }
    }
}
